        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Fantasy_Calender"
        mc:Ignorable="d" DataContext="ViewModels.CalendarViewModel"
        Title="Fantasy Calendar" Height="350" Width="525">
    <Grid>
        <Calendar DisplayDate="{Binding DisplayDate}" Margin="342,149,5,0" Height="170" Width="170">
        </Calendar>
    </Grid>
</Window>
